#!/bin/sh

if command -v zeal.exe  >/dev/null 2>&1; then
  zeal.exe "$@" &
elif command -v "${ProgramFiles}/Zeal"/zeal.exe >/dev/null 2>&1; then
  "${ProgramFiles}/Zeal"/zeal.exe "$@" &
elif command -v "${ProgramFiles} (x86)/Zeal"/zeal.exe >/dev/null 2>&1; then
  "${ProgramFiles} (x86)/Zeal"/zeal.exe "$@" &
else
  echo "No executable zeal.exe  found!"
fi
